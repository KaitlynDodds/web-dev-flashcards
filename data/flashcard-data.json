			{
				"title": "Http Status Codes",
				"cards": [
					{
						"prompt": "Status Code: 100",
						"answer": "Continue",
						"extra": "Interim response indicates that everything is okay so far"
					},
					{
						"prompt": "Status Code: 102",
						"answer": "Processing",
						"extra": "The server has received and is processing the request, but no response is available yet"
					},
					{
						"prompt": "Status Code: 200",
						"answer": "Okay",
						"extra": "request has succeeded"
					},
					{
						"prompt": "Status Code: 301",
						"answer": "Moved Permanently",
						"extra": "the URI of the requested resource has been changed"
					},
					{
						"prompt": "Status Code: 302",
						"answer": "Found",
						"extra": "the URI of requested resource has been changed temporarily"
					},
					{
						"prompt": "Status Code: 304",
						"answer": "Not Modified",
						"extra": "the response has not been modified, so the client can continue to use the same cached version of the response"
					},
					{
						"prompt": "Status Code: 400",
						"answer": "Bad Request",
						"extra": "server could not understand the request due to invalid syntax"
					},
					{
						"prompt": "Status Code: 401",
						"answer": "Unauthorized",
						"extra": "the client must authenticate itself to get the requested response"
					},
					{
						"prompt": "Status Code: 403",
						"answer": "Forbidden",
						"extra": "client does not have access rights to the content (client's identity is known to the server)"
					},
					{
						"prompt": "Status Code: 404",
						"answer": "Not Found",
						"extra": "server can not find requested resource"
					}
				]
			},
